{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string",
      "required": true
    },
    "productSnippet": {
      "type": "richtext"
    },
    "purchaseCost": {
      "type": "integer",
      "required": true
    },
    "sizes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::size.size"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "wishlist": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::wishlist.wishlist",
      "inversedBy": "products"
    },
    "price": {
      "type": "integer",
      "required": true,
      "min": 0
    },
    "p_images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::p-image.p-image",
      "mappedBy": "product"
    },
    "color": {
      "type": "string"
    }
  }
}
